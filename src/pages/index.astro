---
import Layout from '../layouts/Layout.astro';
import { AppConfig } from '../helpers/AppConfig';
import Cover from '@/components/cover/cover';
import GridGates from '@/components/stylesGates/stylesGates';
import TechnicalGates from '@/components/technicalGates/technicalGates';
import ContactGates from '@/components/contact/contact';
import Footer from '@/components/footer/footer';
const { title, description } = AppConfig;
---

<Layout {...{ title, description }}>
	<Cover client:only="react"/>
	<GridGates client:only="react"/>
	<TechnicalGates client:only="react"/>
	<ContactGates client:only="react"/>
	<Footer client:only="react" />
</Layout>

